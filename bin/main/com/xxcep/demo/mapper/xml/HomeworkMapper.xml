<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xxcep.demo.mapper.HomeworkMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xxcep.demo.entity.Homework">
        <id column="homework_id" property="homeworkId" />
        <result column="title" property="title" />
        <result column="begin_date" property="beginDate" />
        <result column="end_date" property="endDate" />
        <result column="content" property="content" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        title, homework_id, begin_date, end_date, content
    </sql>

</mapper>
